{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "chatbot for hotel website",
  "main": "./backend/bin/www",
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "scripts": {
    "start": "node ./backend/bin/www",
